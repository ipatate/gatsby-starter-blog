{
  "version": 2,
  "name": "my-gatsby-project",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    { "src": "/(.+)", "dest": "public/$1" },
    { "src": "/admin", "dest": "admin" },
    {
      "src": "/sw.js",
      "dest": "build/sw.js",
      "headers": { "cache-control": "no-cache" }
    }
  ]
}
